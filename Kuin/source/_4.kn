func main()
	var a: int :: cui@inputInt()
	var b: int :: cui@inputInt()
	var c: int :: cui@inputInt()
	var x: int :: cui@inputInt()
	var r: int :: 0
	for i(0, a)
		for j(0, b)
			for k(0, c)
				if(500 * i + 100 * j + 50 * k = x)
					do r :+ 1
				end if
			end for
		end for
	end for
	do cui@print("\{r}\n")
end func
